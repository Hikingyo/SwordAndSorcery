{
	"name": "SwordAndSorcery",
	"version": "0.0.1",
	"description": "A little game",
	"main": "app.js",
	"man": [
		"docs/README.md"
	],
	"scripts": {
		"dev": "gulp serve",
		"start": "node ./bin/www",
