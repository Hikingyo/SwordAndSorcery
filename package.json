		"cookie-parser": "^1.4.3",
		"ejs": "~2.5.6",
		"express": "latest",
		"express-session": "^1.14.2",
		"express-socket.io-session": "^1.3.1",
		"less-middleware": "^3.0.0",
		"morgan": "^1.7.0",
		"session-file-store": "^1.0.0",
		"socket.io": "latest"
